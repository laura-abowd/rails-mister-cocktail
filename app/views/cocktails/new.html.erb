<nav class="navbar navbar-light bg-light">
  <%= link_to  cocktails_path do %> <i class="fas fa-glass-martini-alt"></i>
  <%end%>
</nav>

<div class="container">
<%= simple_form_for [@cocktail] do |f| %>
  <%= f.input :name %>
  <%= f.input :photo %>
  <%= f.input :photo_cache, as: :hidden %>
  <%= f.submit "Add Cocktail", class: "btn btn-primary" %>
<% end %>
</div>
